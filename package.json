{
  "name": "kefir-store",
  "version": "0.0.1",
  "description": "Basic boilerplate",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeron-diovis/kefir-store"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "mocha",
    "test:watch": "npm test -- --watch",
    "lint": "esw src",
    "lint:watch": "npm run lint -- -w",
    "start": "parallelshell 'npm run test:watch' 'npm run lint:watch'",
    "clean": "rm -rf dist lib",
    "build": "npm run build:transpile && npm run build:bundle",
    "build:bundle": "webpack",
    "build:transpile": "babel src -d lib",
    "prebuild": "npm run lint && npm test && npm run clean"
  },
  "author": "Jeron Diovis <void.jeron.diovis@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.5.0",
    "eslint-watch": "^2.1.14",
    "immutable": "^3.8.1",
    "mocha": "^3.1.0",
    "mock-promises": "^0.7.0",
    "parallelshell": "^2.0.0",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "webpack": "^1.13.2",
    "webpack-error-notification": "^0.1.6",
    "webpack-info-plugin": "^0.1.0"
  },
  "dependencies": {
    "kefir": "^3.5.2",
    "kefir-bus": "^2.2.1",
    "lodash": "^4.16.4"
  }
}
