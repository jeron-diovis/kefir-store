{
  "name": "package-boilerplate",
  "version": "0.0.1",
  "description": "Basic boilerplate",
  "main": "lib/index.js",
  "scripts": {
    "test": "karma start",
    "test:watch": "npm test -- --single-run=false",
    "lint": "esw src",
    "lint:watch": "npm run lint -- -w",
    "start": "npm run test:watch",
    "clean": "rm -rf dist lib",
    "prebuild": "npm test && npm run clean",
    "build:transpile": "babel src -d lib",
    "build:bundle": "webpack",
    "build": "npm run build:transpile && npm run build:bundle"
  },
  "author": "Jeron Diovis <void.jeron.diovis@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "es6-promise": "^3.1.2",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-watch": "^2.1.10",
    "karma": "^0.13.21",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.4",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.14",
    "webpack-error-notification": "^0.1.6",
    "webpack-info-plugin": "^0.1.0"
  }
}
